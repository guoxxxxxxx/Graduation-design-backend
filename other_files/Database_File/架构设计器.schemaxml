<table name="errand" view="standard">
<left>620</left>
<top>1131</top>
<width>243</width>
<height>299</height>
<sql_create_table>CREATE TABLE `errand` (
  `eid` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `uid` int(11) DEFAULT NULL COMMENT '发布订单用户信息 外: user(uid)',
  `euid` int(11) DEFAULT NULL COMMENT '接单用户信息 外: user(uid)',
  `title` varchar(30) DEFAULT NULL COMMENT '订单信息',
  `money` double DEFAULT '0' COMMENT '订单费用, 默认为0',
  `is_achieve` int(11) DEFAULT '0' COMMENT '是否已完成, 0: 未完成; 1: 已完成;',
  `category` varchar(5) DEFAULT NULL COMMENT '类别, 可选值为(快递, 外卖, 打水, 其他)',
  `pubdate` date DEFAULT NULL COMMENT '发布日期',
  `deadline` date DEFAULT NULL COMMENT '截止日期',
  `is_delete` int(11) DEFAULT '0' COMMENT '是否删除, 1: 删除; 0: 未删除; 默认为0',
  PRIMARY KEY (`eid`),
  KEY `uid` (`uid`),
  KEY `euid` (`euid`),
  CONSTRAINT `errand_ibfk_1` FOREIGN KEY (`uid`) REFERENCES `user` (`uid`),
  CONSTRAINT `errand_ibfk_2` FOREIGN KEY (`euid`) REFERENCES `user` (`uid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8</sql_create_table>
</table>
