<table name="user" view="standard">
<left>1069</left>
<top>713</top>
<width>195</width>
<height>282</height>
<sql_create_table>CREATE TABLE `user` (
  `uid` int(11) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `name` varchar(10) DEFAULT NULL COMMENT '姓名',
  `sex` varchar(2) DEFAULT '保密' COMMENT '性别, 仅有(男,女,保密)三个值, 默认为保密',
  `birthday` date DEFAULT '2000-01-01' COMMENT '生日 格式为yyyy-MM-dd',
  `phone` char(11) DEFAULT NULL COMMENT '电话号码, 固定长度为11位',
  `faculty` varchar(20) DEFAULT NULL COMMENT '所属院系',
  `grade` char(4) DEFAULT NULL COMMENT '所属年级, 例如: 2018',
  `major` varchar(20) DEFAULT NULL COMMENT '所属专业',
  `email` varchar(50) DEFAULT NULL COMMENT '邮箱',
  `wechat` varchar(30) DEFAULT NULL COMMENT '微信号',
  `qq` varchar(20) DEFAULT NULL COMMENT 'qq号',
  `is_delete` int(11) DEFAULT '0' COMMENT '是否删除, 1: 删除; 0: 未删除; 默认为0',
  PRIMARY KEY (`uid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8</sql_create_table>
</table>
